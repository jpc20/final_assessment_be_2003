<h1><%= @doctor.name %></h1>
<h3>Specialty: <%= @doctor.specialty %></h3>
<h3>Education: <%= @doctor.education %></h3>
<h3>Hospital: <%= @doctor.hospital.name %></h3>

<section class="patients">
  <h3>Patients:</h3>
  <% @doctor.patients.each do |patient| %>
    <p><%= patient.name %></p>
  <% end %>
</section>
